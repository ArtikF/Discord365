<UserControl x:Class="Discord365.UI.User.Screens.ScreenSearch"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Discord365.UI.User.Screens"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock Foreground="#7FFFFFFF" Text="Search" FontSize="22" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,132,0,288"></TextBlock>
        <TextBlock Foreground="#7FFFFFFF" Text="You should know, there is no search in Discord 365 =(" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,162,0,269"></TextBlock>
        <TextBox x:Name="tbSearchBox" HorizontalAlignment="Center" Height="25" Margin="0,186,0,239" TextWrapping="Wrap" VerticalAlignment="Center" Width="368" Background="#19000000" Foreground="#BFFFFFFF" BorderBrush="#3F000000" FontSize="16"/>
    </Grid>
</UserControl>
