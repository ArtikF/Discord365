             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Discord365.UI.User.CustomContent.UserSettings"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock Text="Log out from your account?" Foreground="#FFFFFFFF" Margin="16" HorizontalAlignment="Left" VerticalAlignment="Top"></TextBlock>
        <Button HorizontalAlignment="Left" VerticalAlignment="Top" Width="85" Margin="16,37,0,0" Click="Button_Click">Yes</Button>
    </Grid>
