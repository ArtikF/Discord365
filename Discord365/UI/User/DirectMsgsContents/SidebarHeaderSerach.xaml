<UserControl x:Class="Discord365.UI.User.DirectMsgsContents.SidebarHeaderSerach"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Discord365.UI.User.DirectMsgsContents"
             mc:Ignorable="d" 
             Width="216" Height="32">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Discord365.Resources;component/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <TextBox x:Name="tbSearchText" Margin="6" Foreground="White" TextChanged="TbSearchText_TextChanged"/>
    </Grid>
</UserControl>
