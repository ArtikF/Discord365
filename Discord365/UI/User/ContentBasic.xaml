<UserControl x:Class="Discord365.UI.User.ContentBasic"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Discord365.UI.User"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Foreground="{x:Null}">
    <Grid Background="#14FFFFFF">
        <Grid x:Name="GridHeader" HorizontalAlignment="Stretch" Height="49" VerticalAlignment="Top"/>
        <Grid x:Name="GridContent" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,51,0,0"></Grid>
        <local:ContentSeparator VerticalAlignment="Top" Margin="0,49,0,0" Height="2"></local:ContentSeparator>
    </Grid>
</UserControl>
